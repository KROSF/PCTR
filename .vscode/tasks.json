{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run google java format",
      "type": "shell",
      "command": "google-java-format --replace ${file}",
      "problemMatcher": []
    },
    {
      "label": "Generate javadocs practicas",
      "type": "shell",
      "options": {
          "cwd": "${workspaceRoot}/practicas"
      },
      "command" : "mvn javadoc:javadoc && cp -r target/site/apidocs/ ../docs",
      "problemMatcher": []
    }
  ]
}
